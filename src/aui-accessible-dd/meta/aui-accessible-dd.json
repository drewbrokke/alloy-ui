{
    "aui-accessible-dd": {
        "use": [
            "aui-accessible-dd-delegate",
            "aui-accessible-dd-drag",
            "aui-accessible-dd-drop"
        ]
    },

    "aui-accessible-dd-delegate": {
        "requires": [
            "aui-accessible-dd-drag",
            "aui-component",
            "aui-event-delegate-change",
            "dd-delegate",
            "dom",
            "event-focus",
            "event-key",
            "node-focusmanager"
        ]
    },

    "aui-accessible-dd-drag": {
        "requires": [
            "aui-component",
            "dd-drag",
            "dom",
            "event-focus",
            "event-key"
        ]
    },

    "aui-accessible-dd-drop": {
        "requires": [
            "aui-component",
            "dd-drop",
            "dom",
            "event-focus",
            "event-key"
        ]
    }
}